{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": false
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.glyphMargin": false,
  "editor.occurrencesHighlight": false,
  "editor.renderIndentGuides": false,
  "editor.rulers": [80],
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordBasedSuggestions": false,
  "editor.wordWrap": "on",
  "emmet.excludeLanguages": ["markdown", "typescript", "javascript"],
  "emmet.showAbbreviationSuggestions": false,
  "emmet.showExpandedAbbreviation": "never",
  "eslint.migration.2_x": "off",
  "explorer.autoReveal": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/env/**": true,
    "**/node_modules/**": true,
    "**/venv/**": true,
    "env-*": true
  },
  "git.enableSmartCommit": true,
  "javascript.suggest.names": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": false,
  "jsannotations.enabled": true,
  "openInTerminal.app": "iTerm",
  "problems.decorations.enabled": true,
  "search.exclude": {
    "**/.cache": true,
    "**/android": true,
    "**/bower_components": true,
    "**/build": true,
    "**/build-chrome": true,
    "**/build-firefox": true,
    "**/coverage": true,
    "**/dist": true,
    "**/extension-build": true,
    "**/gsp-dist": true,
    "**/ios": true,
    "**/node_modules": true,
    "**/public": true,
    "**/vendor": true
  },
  "search.useIgnoreFiles": false,
  "terminal.integrated.shell.osx": "zsh",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "[Subliminal Next]": {
      "gitDecoration.ignoredResourceForeground":"#76879670",
      "scrollbarSlider.background": "#76879670"
    }
  },
  "workbench.colorTheme": "Nord",
  "workbench.editor.tabCloseButton": "off",
  "workbench.iconTheme": "ayu",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.visible": false
}
